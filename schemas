CREATE TABLE `users` (
  `FirstName` char(33) DEFAULT NULL,
  `LastName` char(33) DEFAULT NULL,
  `Name` char(15) DEFAULT NULL,
  `Active` tinyint(1) DEFAULT NULL,
  KEY `IFK_name` (`name`)
);

) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

